{
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "python.analysis.extraPaths": [],
  "python.analysis.importFormat": "relative",
  "python.autoComplete.extraPaths": [],
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredSettings": [],
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "underline",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.activityBar.location": "bottom",
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "window.commandCenter": false,
  "window.menuBarVisibility": "toggle",
  "window.newWindowDimensions": "inherit",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "codeium.enableCodeLens": "hide_x",
  "codeium.enableConfig": {
    "*": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.defaultFoldingRangeProvider": "esbenp.prettier-vscode",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.emptySelectionClipboard": false,
  "editor.fontFamily": "Geist Mono, JetBrains Mono, Fira Code",
  "editor.guides.indentation": false,
  "editor.lineNumbers": "off",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fit",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.scrollbar.horizontalScrollbarSize": 4,
  "editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.smoothScrolling": true,
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'calt', 'dlig'",
  "editor.snippetSuggestions": "top",
  "eslint.format.enable": true,
  "editor.fontWeight": "400",
  "breadcrumbs.enabled": false,
  "scm.diffDecorations": "none",
  "editor.hover.delay": 1500,
  "editor.hover.enabled": true,
  "workbench.tips.enabled": false,
  "editor.colorDecorators": false,
  "workbench.startupEditor": "none",
  "editor.lightbulb.enabled": "off",
  "editor.selectionHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "problems.decorations.enabled": false,
  "editor.renderControlCharacters": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.wordWrapColumn": 120,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#5c6370",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword", "storage.type", "storage.modifier"],
        "settings": {
          "foreground": "#c678dd",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "variable",
        "settings": {
          "foreground": "#e06c75"
        }
      },
      {
        "scope": "variable.other.readwrite",
        "settings": {
          "foreground": "#d19a66"
        }
      },
      {
        "scope": "variable.other.constant",
        "settings": {
          "foreground": "#e5c07b"
        }
      },
      {
        "scope": "variable.other.object",
        "settings": {
          "foreground": "#56b6c2"
        }
      },
      {
        "scope": "variable.other.property",
        "settings": {
          "foreground": "#98c379"
        }
      },
      {
        "scope": "variable.other.object.property",
        "settings": {
          "foreground": "#61afef"
        }
      },
      {
        "scope": "variable.other.object.property.dom",
        "settings": {
          "foreground": "#c678dd"
        }
      },
      {
        "scope": "variable.language",
        "settings": {
          "foreground": "#e06c75",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "variable.parameter",
        "settings": {
          "foreground": "#d19a66",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["entity.name.function", "support.function"],
        "settings": {
          "foreground": "#61afef"
        }
      },
      {
        "scope": "entity.name.method",
        "settings": {
          "foreground": "#98c379"
        }
      },
      {
        "scope": ["entity.name.type", "entity.other.inherited-class"],
        "settings": {
          "foreground": "#e5c07b"
        }
      },
      {
        "scope": "string",
        "settings": {
          "foreground": "#98c379"
        }
      },
      {
        "scope": ["constant.numeric", "constant.language.boolean"],
        "settings": {
          "foreground": "#d19a66"
        }
      },
      {
        "scope": ["constant.language", "support.constant"],
        "settings": {
          "foreground": "#56b6c2",
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "punctuation.definition.string",
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end"
        ],
        "settings": {
          "foreground": "#98c379"
        }
      },
      {
        "scope": ["punctuation", "meta.brace"],
        "settings": {
          "foreground": "#abb2bf"
        }
      },
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#56b6c2"
        }
      },
      {
        "scope": ["meta.tag", "punctuation.definition.tag"],
        "settings": {
          "foreground": "#e06c75"
        }
      },
      {
        "scope": ["entity.name.tag", "meta.tag.sgml"],
        "settings": {
          "foreground": "#e06c75"
        }
      },
      {
        "scope": "markup.inline.raw",
        "settings": {
          "foreground": "#98c379"
        }
      },
      {
        "scope": "invalid",
        "settings": {
          "foreground": "#f44747"
        }
      },
      {
        "scope": "markup.bold",
        "settings": {
          "foreground": "#d19a66",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.heading",
        "settings": {
          "foreground": "#61afef",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "foreground": "#c678dd",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "meta.object-literal.key",
        "settings": {
          "foreground": "#56b6c2"
        }
      },
      {
        "scope": "support.type.property-name",
        "settings": {
          "foreground": "#98c379"
        }
      },
      {
        "scope": "support.constant.math",
        "settings": {
          "foreground": "#e5c07b"
        }
      },
      {
        "scope": "support.type.primitive",
        "settings": {
          "foreground": "#d19a66"
        }
      }
    ]
  },
  "search.exclude": {
    "**/vendor/{[^l],?[^ai]}*": true,
    "**/public/{[^i],?[^n]}*": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/_ide_helper.php": true,
    "**/composer.lock": true,
    "**/package-lock.json": true,
    "storage": true,
    ".phpunit.result.cache": true
  },
  "search.useIgnoreFiles": false,
  "turboConsoleLog.includeFileNameAndLineNum": true,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.wrapLogMessage": true,
  "workbench.editorAssociations": {
    "*.csv": "default"
  },
  "workbench.colorTheme": "React Theme"
}
